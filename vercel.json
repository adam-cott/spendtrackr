{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/send-email.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/verify-pin.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "manifest.json",
      "use": "@vercel/static"
    },
    {
      "src": "icon.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/analyze.py"
    },
    {
      "src": "/api/send-email",
      "dest": "/api/send-email.py"
    },
    {
      "src": "/api/verify-pin",
      "dest": "/api/verify-pin.py"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/icon.png",
      "dest": "/icon.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
